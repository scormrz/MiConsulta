<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/fondo1"
    tools:context="com.scorm.redfi.miconsulta.ConsultarCita">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="201dp"
        android:layout_height="72dp"
        android:layout_below="@+id/button9"
        android:layout_centerHorizontal="true"
        android:text="Tus Citas"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <RelativeLayout
        android:layout_width="354dp"
        android:layout_height="492dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.195"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

        <ListView
            android:id="@+id/lista"
            style="@android:style/Widget.Holo.Light.ExpandableListView"
            android:layout_width="match_parent"
            android:layout_height="380dp"
            android:textAlignment="center"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="14dp" />

        <Button
            android:id="@+id/button9"
            style="@android:style/Widget.Material.Button.Colored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:onClick="consul"
            android:text="Ver tus Citas" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
